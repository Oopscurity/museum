{
  "name": "museum",
  "version": "0.0.1-SNAPSHOT",
  "description": "Virtual museum of computer science",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oopscurity/museum.git"
  },
  "main": "src/index.jsx",
  "scripts": {
    "start": "NODE_ENV=development node -r 'babel-register' ./src/server/index.js --progress --colors --profile",
    "start:prod": "NODE_ENV=production node ./build/server.js --progress --colors --profile",
    "build": "npm run build:client:prod && npm run build:server:prod",
    "build:client:prod": "NODE_ENV=development webpack -p --config ./webpack/webpack.client.prod.config.js",
    "build:server:prod": "NODE_ENV=production webpack -p --config ./webpack/webpack.server.prod.config.js",
    "test": "mocha",
    "lint": "node node_modules/.bin/eslint '+(src|test)/**'",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover --include-all-sources _mocha -- -R spec",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "travis": "npm run lint && npm run coverage && npm run coveralls",
    "clean": "rm -rf build"
  },
  "author": "Artem Kravchenko",
  "private": true,
  "license": "",
  "dependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "classnames": "^2.2.4",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extendable-immutable": "^1.0.13",
    "history": "^4.3.0",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.4",
    "konva": "^1.2.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.11.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-helmet": "^3.1.0",
    "react-konva": "^1.0.5",
    "react-markdown": "^2.4.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "redux": "^3.5.2",
    "redux-immutablejs": "0.0.8",
    "redux-thunk": "^2.0.1",
