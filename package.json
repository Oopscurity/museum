    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-survivejs-kanban": "^0.3.3",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "coveralls": "^2.11.9",
    "dirty-chai": "^1.2.2",
    "eslint": "^2.9.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-mocha": "^2.2.0",
